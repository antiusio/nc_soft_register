<Window x:Class="MainWin.Windows.AddProxys"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MainWin.Windows"
        mc:Ignorable="d"
        Title="AddProxys" Height="315.287" Width="516.242">
    <Grid>
        <Menu HorizontalAlignment="Stretch" VerticalAlignment="Top">
            <MenuItem Header="Меню">
                <MenuItem Header="Загрузить ssh туннели" Click="UploadSSHTunnelsMenuItem_Click"/>
            </MenuItem>
        </Menu>
        <TextBox Margin="0,20,0,0" TextWrapping="Wrap" AcceptsReturn="True" Text="{Binding TextProxys, UpdateSourceTrigger=PropertyChanged}"/>
    </Grid>
</Window>
